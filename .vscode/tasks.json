{
  // See https://go.microsoft.com/fwlink/?LinkId=733558
  // for the documentation about the tasks.json format
  "version": "2.0.0",
  "tasks": [
    {
      "label": "trace turbo",
      "type": "shell",
      "command": "node --allow-natives-syntax --trace-turbo --no-turbo-inlining ${file}",
      "presentation": {
        "clear": true
      },
      "problemMatcher": []
    },
    {
      "label": "run",
      "type": "shell",
      "command": "node ${file}",
      "presentation": {
        "clear": true
      },
      "problemMatcher": []
    },
    {
      "label": "clean turbo results",
      "type": "shell",
      "command": "rm turbo-*.cfg turbo-*.json",
      "presentation": {
        "clear": true
      },
      "problemMatcher": []
    },
    {
      "type": "npm",
      "script": "tsc",
      "problemMatcher": [
        "$tsc"
      ],
      "label": "npm: tsc",
      "detail": "tsc"
    }
  ]
}